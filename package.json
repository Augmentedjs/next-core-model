{
  "name": "next-core-model",
  "version": "2.6.0",
  "description": "The Augmented.js Next - Model & Collection.",
  "main": "dist/core-next-model.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --progress",
    "lint": "eslint ./src/index.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "readme": "./node_modules/documentation/bin/documentation.js readme ./src/index.js -f md --section=API",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/next-core-model.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "documentation": "^11.0.1",
    "eslint": "^6.0.0",
    "eslint-config-kentcdodds": "^14.3.3",
    "foodoc": "0.0.9",
    "jsdoc": "^3.6.2",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "mocha-jsdom": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "lodash.clone": "^4.5.0",
    "lodash.defaults": "^4.2.0",
    "lodash.defer": "^4.1.0",
    "lodash.escape": "^4.0.1",
    "lodash.has": "^4.5.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.iteratee": "^4.7.0",
    "lodash.result": "^4.5.2",
    "next-core-object": "^2.6.0",
    "next-core-utilities": "^2.6.1",
    "next-core-validation": "^2.5.4"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/next-core-model/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
